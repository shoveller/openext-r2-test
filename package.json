{
  "name": "openext-r2-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "preview:remote": "opennextjs-cloudflare build && opennextjs-cloudflare preview --remote",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "prettier": "prettier --write \"**/*.{ts,tsx,cjs,mjs,json,html,css,js,jsx}\" --cache",
    "eslint": "eslint --fix --ignore-pattern .gitignore --cache --cache-location node_modules/.cache/eslint/.eslintcache .",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "format": "run-s typecheck prettier eslint"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.14.4",
    "next": "15.5.9",
    "react": "19.1.4",
    "react-dom": "19.1.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "prettier-plugin-classnames": "^0.8.6",
    "prettier-plugin-css-order": "^2.2.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.52.0",
    "wrangler": "^4.58.0"
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184"
}
